print('Calculadora de partidas Rankeadas')

TotalVitorias = int(input("Insira a quantidade de vitória do seu herói: "))
TotalDerrotas = int(input("Insira a quantidade de derrotas do seu herói: "))

saldo = TotalVitorias - TotalDerrotas

def Level (saldo):
    if saldo <= 10:
        print ("O Herói tem saldo de", saldo , "vitórias, e está no nível de Ferro")
    
    elif 11 <= saldo <= 20:
        print ("O Herói tem saldo de", saldo , "vitórias, e está no nível de Bronze")
    
    elif 21 <= saldo <= 50:
        print ("O Herói tem saldo de", saldo , "vitórias, e está no nível de Prata")
        
    elif 51 <= saldo <= 70:
        print ("O Herói tem saldo de", saldo ,"vitórias, e está no nível de Ouro")
    
    elif 71 <= saldo <= 80:
        print ("O Herói tem saldo de", saldo , "vitórias, e está no nível de Platina")
    
    elif 81 <= saldo <= 90:
        print ("O Herói tem saldo de", saldo , "vitórias, e está no nível de Ascendente")
        
    elif 91 <= saldo <=100:
        print ("O Herói tem saldo de", saldo , "vitórias, e está no nível de Imortal")
        
    elif saldo >100:
        print ("O Herói tem saldo de", saldo , "vitórias, e está no nível de Radiante")  
        
    else:
        print ()
        
Level (saldo)
